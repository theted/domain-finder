    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "body-parser": "^1.20.2",
    "bullmq": "^5.8.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "lucide-react": "^0.407.0",
    "openai": "^4.52.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
