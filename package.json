{
  "name": "domain-finder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start-server": "bun run backend/server.ts",
    "start-worker": "bun run backend/worker.ts",
    "start-server-and-worker": "concurrently \"bun run start-server\" \"bun run start-worker\"",
    "start-dev-environment": "concurrently \"bun run start-server\" \"bun run start-worker\" \"bun run dev\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test-ci": "DEBUG=true start-server-and-test start-server-and-worker http://localhost:5000 test"
  },
  "dependencies": {
